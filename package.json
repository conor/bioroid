{
  "name": "bioroid",
  "version": "1.0.0",
  "description": "TypeScript library that emulates Clojure Replicant for Hiccup-style markup rendering",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "example": "npm run build && python3 -m http.server 8000"
  },
  "keywords": ["hiccup", "virtual-dom", "replicant", "typescript", "clojure"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "jsdom": "^22.1.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "files": ["dist"]
}
